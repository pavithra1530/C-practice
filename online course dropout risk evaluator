#include <stdio.h>
int main() {
    int N;
    int i = 0;
    int hours;
    int inactive = 0;
    scanf("%d", &N);
    while(i < N) {
        scanf("%d", &hours);

        if(hours == 0) {
            inactive++; // count inactive week
        }
        i++;
    }
    printf("Inactive Weeks: %d\n", inactive);
    if(inactive >= 3) {
        printf("Risk Status: High");
    } else {
        printf("Risk Status: Low");
    }
    return 0;
}
