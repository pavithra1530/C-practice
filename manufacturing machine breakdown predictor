#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    int i = 0;
    int unsafeCount = 0;
    int consecutiveUnsafe = 0;
    int breakdownPoint = -1;
    while(i < n) {
        if(arr[i] > 70) {
            unsafeCount++;
            consecutiveUnsafe++;
            if(consecutiveUnsafe == 3 && breakdownPoint == -1) {
                breakdownPoint = i + 1;
            }
        } else {
            consecutiveUnsafe = 0;
        }
        i++;
    }
    if(breakdownPoint != -1) {
        printf("Breakdown At Reading: %d\n", breakdownPoint);
    } else {
        printf("Breakdown At Reading: Not Occurred\n");
    }
    printf("Unsafe Readings: %d\n", unsafeCount);
    return 0;
}
