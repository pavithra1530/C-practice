#include <stdio.h>
int main() {
    int data,N,i=1,use,day=-1,over=0;
    scanf("%d %d",&data,&N);
    while (i<=N){
        scanf("%d",&use);
        data -= use;
        if (data<=0) {
            day=i;
            if (data<0)
                over=-data;
                break;
            }
            i++;
    }
    if (day== -1)
        printf("Exhausted Day: Not Exhausted\nOverused Data:0");
    else
        printf("Exhausted Day: %d\n %d" ,day,over);
    return 0;
}