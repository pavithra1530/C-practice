#include <stdio.h>
int main() {
    int n,temp,digit,i,fact,sum=0;;
    scanf("%d",&n);
    temp=n;
    for (;n>0;n/=10) {
        digit=n%10;
        fact=1;
        for (i=1;i<=digit;i++)
            fact *=i;
        sum+=fact;
    }
    if (sum==temp) {
        printf("Yes");
    }
    else {
        printf("No");
    }
    return 0;
}