#include <stdio.h>
int main() {
    int choice;
    int a,b;
    scanf("%d",&choice);

    switch(choice) {
        case 1:
            scanf("%d%d",&a,&b);

            printf("sum= %d",a+b);
        case 2:
            scanf("%d%d",&a,&b);
            printf("diff= %d",a-b);
            break;
        case 3:
            scanf("%d%d",&a,&b);
            printf("multi= %d",a*b);
            break;
        case 4:
            scanf("%d%d",&a,&b);
            printf("divide= %d",a/b);
            break;
        default:
            printf("default");
            break;
        }
        return 0;
    }