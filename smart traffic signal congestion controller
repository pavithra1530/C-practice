#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    int vehicles[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &vehicles[i]);
    }
    int i = 0;
    int congestionMinutes = 0;
    int currentStreak = 0;
    int longestStreak = 0;
    while(i < n) {
        if(vehicles[i] > 20) {
            congestionMinutes++;
            currentStreak++;
            if(currentStreak > longestStreak)
            {
                longestStreak = currentStreak;
            }
        } else {
            currentStreak = 0;
        }
        i++;
    }
    printf("Congestion Minutes: %d\n", congestionMinutes);
    printf("Longest Congestion Streak: %d\n", longestStreak);
    return 0;
}
