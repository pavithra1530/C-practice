#include <stdio.h>
int main() {
    int n;
    scanf("%d",&n);
    int noise;
    int i=0;
    int violations=0;
    int longestStreak=0;
    int currentStreak=0;
    while (i<n) {
        scanf("%d",& noise);
        if (noise>70) {
            violations++;
            currentStreak++;
            if (currentStreak>longestStreak) {
                longestStreak=currentStreak;
            }
        }
        else {
            currentStreak=0;
        }
        i++;
    }
    printf("Noice Violations: %d\n",violations);
    printf("Longest violations Streak: %d\n", longestStreak);
    return 0;
}